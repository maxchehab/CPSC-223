all: Exception.cpp Clientprogram.cpp TreeNode.cpp BinaryTree.cpp  Item.cpp Key.cpp
	clang++ Exception.cpp Clientprogram.cpp TreeNode.cpp BinaryTree.cpp  Item.cpp Key.cpp -o Golink -std=c++11
	./Golink

build: Exception.cpp Clientprogram.cpp TreeNode.cpp BinaryTree.cpp  Item.cpp Key.cpp
	clang++ Exception.cpp Clientprogram.cpp TreeNode.cpp BinaryTree.cpp  Item.cpp Key.cpp -o Golink -std=c++11
clean: 
	$(RM) ./Golink

run: Golink
	./Golink
